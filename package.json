{
  "name": "fizz-burger-challenge",
  "version": "1.0.0",
  "description": "Fiz-Burger Challenge",
  "main": "src/index.js",
  "scripts": {
    "start": "serverless offline start",
    "dev": "nodemon --watch src --exec \"serverless offline start\"",
    "test": "npx mocha 'src/**/*/*.test.js' --recursive",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker": "docker-compose up --build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm test -- --silent"
    ]
  },
  "keywords": [
    "fizzmod",
    "burger",
    "ecommerce",
    "challenge"
  ],
  "author": "Fizzmod",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.5.0",
    "dotenv": "^16.5.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha": "^10.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "serverless": "^4.12.0",
    "serverless-offline": "^14.4.0",
    "sinon": "^20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.787.0",
    "@aws-sdk/client-lambda": "^3.787.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.2",
    "zod": "^3.24.2"
  }
}
